---
import SectionTitle from "../SectionTitle.astro";
---

<section id="powered" class="w-full flex flex-col gap-8">
    <SectionTitle>powered by</SectionTitle>
    <div class="flex gap-5 items-center justify-between px-16">
        <img src="/ts-logo.png" alt="typescript" /><img
            src="/astro-logo.png"
            alt="astro"
        /><img src="/react-logo.png" alt="reactJS" /><img
            src="/tailwind-logo.png"
            alt="tailwindCSS"
        /><img src="/qr-logo.png" alt="react-qr-code" />
    </div>
</section>

<style>
    #powered img {
        height: 32px;
    }
</style>
